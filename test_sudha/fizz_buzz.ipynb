    "def fizz_buzz():\n",
    "    num=int(input(\"Enter any number: \"))\n",
    "    if num%3==0:\n",
    "        return(\"fizz\")\n",
    "    if num%5==0:\n",
    "        return(\"buzz\")\n",
    "    if num%5==0 and num%3==0:\n",
    "        return(\"fizzbuzz\")\n",
    "    else:\n",
    "        return(num)\n",
    "    \n",
    "fizz_buzz()\n"
